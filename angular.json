{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ecommerce": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/ecommerce",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/img/favicon.png",
              "src/assets/img"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/vendor/bootstrap/css/bootstrap.min.css",
              "src/assets/vendor/lightbox2/css/lightbox.min.css",
              "src/assets/vendor/nouislider/nouislider.min.css",
              "src/assets/vendor/bootstrap-select/css/bootstrap-select.min.css",
              "src/assets/vendor/owl.carousel2/assets/owl.carousel.min.css",
              "src/assets/vendor/owl.carousel2/assets/owl.theme.default.css",
              "src/assets/css/style.default.css",
              "src/assets/css/custom.css",
              "https://use.fontawesome.com/releases/v5.7.1/css/all.css"
            ],
            "scripts": [
              "src/assets/vendor/jquery/jquery.min.js",
              "src/assets/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/vendor/lightbox2/js/lightbox.min.js",
              "src/assets/vendor/nouislider/nouislider.min.js",
              "src/assets/vendor/bootstrap-select/js/bootstrap-select.min.js",
              "src/assets/vendor/owl.carousel2/owl.carousel.min.js",
              "src/assets/vendor/owl.carousel2.thumbs/owl.carousel2.thumbs.min.js",
              "src/assets/js/front.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ecommerce:build:production"
            },
            "development": {
              "buildTarget": "ecommerce:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ecommerce:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
